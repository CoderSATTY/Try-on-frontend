{
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/index.py" },
    { "source": "/gradio/(.*)", "destination": "/api/index.py" },
    { "source": "/gradio", "destination": "/api/index.py" },
    { "source": "/(.*)", "destination": "/static/$1" },
    { "source": "/", "destination": "/static/index.html" }
  ]
}